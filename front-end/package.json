    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^5.0.1",
    "chai": "^4.1.2",
    "chromedriver": "^76.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "cssnano": "^4.1.4",
    "eslint": "^5.2.0",
    "eslint-plugin-compat": "^2.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.6.1",
    "fetch-mock": "^7.0.7",
