  "version": "0.0.1",
  "scripts": {
    "build": "GIT_COMMIT=${GIT_COMMIT:-$(git rev-parse HEAD)} NODE_ENV=${NODE_ENV:-production} webpack",
    "start": "script/dev-server.js",
    "test": "NODE_ENV=${NODE_ENV:-test} bin/ci",
    "test:unit": "NODE_ENV=${NODE_ENV:-test} karma start config/karma.js --single-run",
    "test:watch": "NODE_ENV=${NODE_ENV:-test} karma start config/karma.js --watch",
    "test:e2e": "NODE_ENV=${NODE_ENV:-test} SERVER_PORT=9091 script/test-e2e.js",
    "lint": "yarn run eslint && yarn run stylelint",
    "eslint": "eslint src/ test/",
    "stylelint": "stylelint 'src/**/*.scss'"
  },
  "dependencies": {
    "preact": "^8.2.9"
  },
  "devDependencies": {
