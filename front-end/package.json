    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^4.0.0-rc.3",
    "mini-css-extract-plugin": "^0.5.0",
